block('manufactura')(
    js()(true),
    tag()('a'),
    attrs()(function() {
        return this.extend(applyNext(), {
            href : this.ctx.url || 'http://factory.mn/',
            title : this.ctx.title || 'Мануфактура',
            alt : this.ctx.title || 'Мануфактура',
            target : '_blank'
        })
    }),
    content()(function() {
        return this.ctx.content || 'Сделано в «Мануфактуре»'
    })
)


